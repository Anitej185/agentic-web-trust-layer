# Financial Intelligence System Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI API Key (optional - fallback if Groq not available)
OPENAI_API_KEY=your-openai-api-key-here

# Groq API Key (recommended for ultra-fast responses)
# Get your free API key at: https://console.groq.com/keys
GROQ_API_KEY=your-groq-api-key-here

# Optional: Financial Data API Keys (for enhanced features)
# Alpha Vantage API Key (free tier available)
ALPHA_VANTAGE_API_KEY=your-alpha-vantage-key-here

# Finnhub API Key (free tier available)
FINNHUB_API_KEY=your-finnhub-api-key-here

# Redis Configuration (optional - for caching)
REDIS_URL=redis://localhost:6379
REDIS_CACHE_TTL=300

# System Configuration
DEBUG=False
LOG_LEVEL=INFO
MAX_CACHE_SIZE=100
API_TIMEOUT=30
